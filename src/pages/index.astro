---
import { Content } from "../contents/index.mdx";
import { Content as ContentDates } from "../contents/dates.mdx";
import BaseLayout from "@/layouts/BaseLayout.astro";
import ContentLayout from "@/layouts/ContentLayout.astro";
import PeopleList from "@/components/PeopleList.astro";
import invited_speakers from "../assets/invited_speakers.json";
---

<BaseLayout title='Home'>
  <ContentLayout>
    <Content />
  </ContentLayout>

  <PeopleList title='Invited Speakers' people={invited_speakers} />

  <ContentLayout>
    <ContentDates />
  </ContentLayout>
</BaseLayout>
